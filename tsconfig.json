{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "module": "CommonJS",
    "target": "ES2021",
    "baseUrl": "./",
    "rootDir": "..",
    "paths": {
      "dto/*": ["src/dto/*.dto"],
      "@icons": ["src/config/enum/icons.enum"],
      "components/*": ["src/components/*"],
      "@templates/*": ["src/templates/*"],
      "axios-config": ["src/api/axios.config"],
      "notifications/*": ["src/notifications/*"],
      "@config/validation": ["src/config/validation/yup.config"],
      "@config/date": ["src/config/date.config"],
      "router": ["router/Router"],
      "@store/*": ["src/stores/*.mobx"],
      "fonts/*": ["assets/fonts/*"],
      "images/*": ["assets/images/*"],
      "@animated/*": ["src/animated/*"],
      "@interfaces/*": ["src/interfaces/*.interface"]
    }
  }
}
